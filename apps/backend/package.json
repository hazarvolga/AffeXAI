{
  "name": "affexai-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && npm run compile:templates",
    "compile:templates": "tsx scripts/compile-email-templates.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "./scripts/start-dev.sh",
    "cleanup": "./scripts/cleanup.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:generate": "npm run typeorm -- -d ./src/database/data-source.ts migration:generate",
    "typeorm:migration:run": "npm run typeorm -- -d ./src/database/data-source.ts migration:run",
    "typeorm:migration:revert": "npm run typeorm -- -d ./src/database/data-source.ts migration:revert",
    "seed:certificates": "ts-node src/modules/certificates/seeds/seed-templates.ts",
    "seed:users": "ts-node src/database/seeds/seed-users-roles.ts",
    "seed:tickets": "ts-node src/database/seeds/seed-tickets.ts",
    "seed:critical-settings": "tsx src/database/seeds/run-critical-settings.ts",
    "seed:email-builder": "ts-node src/scripts/seed-email-builder-templates.ts",
    "seed:ticket-templates": "ts-node src/scripts/seed-ticket-email-templates.ts",
    "seed:reusable-content": "ts-node src/database/seeds/run-reusable-content.ts",
    "seed:homepage-sections": "tsx src/database/seeds/run-homepage-sections.ts",
    "seed:cms-templates": "tsx src/modules/cms/seeds/run-templates.ts",
    "seed:theme-settings": "ts-node src/database/seeds/run-theme-settings.ts",
    "seed:migrate-pages": "tsx src/database/seeds/run-migrate-public-pages.ts",
    "update:empty-templates": "tsx src/scripts/update-empty-templates.ts",
    "seed:remaining-pages": "tsx src/database/seeds/run-seed-remaining-pages.ts"
  },
