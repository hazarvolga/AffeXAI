\f2\b0\fs23\fsmilli11700 \cf4 \cb5 \strokec4 # Otomatik backup script (Coolify cronjob ile \'e7al\uc0\u305 \u351 t\u305 r)\
#!/bin/bash\
# backup-database.sh\
\
DATE=$(date +%Y%m%d_%H%M%S)\
BACKUP_DIR="/backups"\
\
# PostgreSQL backup\
pg_dump -h $DATABASE_HOST -U $DATABASE_USERNAME $DATABASE_NAME > $BACKUP_DIR/db_backup_$DATE.sql\
\
# Upload to S3 (optional but recommended)\
aws s3 cp $BACKUP_DIR/db_backup_$DATE.sql s3://your-bucket/backups/\
\
# Keep only last 7 days\
find $BACKUP_DIR -name "db_backup_*.sql" -mtime +7 -delete\
\
echo "Backup completed: db_backup_$DATE.sql"\
\pard\pardeftab720\partightenfactor0

