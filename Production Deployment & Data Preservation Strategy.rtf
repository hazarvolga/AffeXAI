\f0\b\fs26 \cf2 \cb3 \strokec2 TypeORM Migration Workflow:
\f2\b0 \
\pard\pardeftab720\partightenfactor0

\fs23\fsmilli11700 \cf4 \cb5 \strokec4 // 1. Development'ta yeni \'f6zellik ekle\
// \'d6rnek: CMS'e yeni bir alan ekledin\
\
// 2. Migration olu\uc0\u351 tur\
npm run typeorm:migration:generate -- src/database/migrations/AddNewFieldToCms\
\
// 3. Migration dosyas\uc0\u305  otomatik olu\u351 ur\
// src/database/migrations/1234567890-AddNewFieldToCms.ts\
export class AddNewFieldToCms1234567890 implements MigrationInterface \{\
  public async up(queryRunner: QueryRunner): Promise<void> \{\
    // Yeni s\'fctun ekle (mevcut veri korunur)\
    await queryRunner.addColumn('pages', new TableColumn(\{\
      name: 'new_field',\
      type: 'varchar',\
      isNullable: true, // \'d6NEML\uc0\u304 : Mevcut veriler null olacak\
    \}));\
  \}\
\
  public async down(queryRunner: QueryRunner): Promise<void> \{\
    // Rollback i\'e7in\
    await queryRunner.dropColumn('pages', 'new_field');\
  \}\
\}\
\
// 4. Local'de test et\
npm run typeorm:migration:run\
\
// 5. Git'e commit\
git add src/database/migrations/\
git commit -m "feat: add new field to CMS pages"\
\
// 6. Sunucuda sadece yeni migration'\uc0\u305  \'e7al\u305 \u351 t\u305 r\
# Sunucuda\
npm run typeorm:migration:run\
# Bu sadece yeni migration'lar\uc0\u305  \'e7al\u305 \u351 t\u305 r\u305 r, eski veri korunur\
